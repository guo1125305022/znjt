<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="et_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ems">10</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/login_info_et_bg</item>
    </style>


    <style name="default_text_view_style">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="TextTitle"></style>

    <style name="TextContent"></style>

    <style name="MenuItem"></style>

    <style name="MenuTitle"></style>

    -

    <style name="tv_show_style">
        <item name="android:textSize">20sp</item>

    </style>

</resources>
